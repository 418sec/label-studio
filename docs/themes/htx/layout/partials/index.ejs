<div class="main__block">
  <div class="container">
    <div class="outer__main">
      <div class="main__content">
        <div class="left__content">
          <h1>Open Source <br>
          Data  <span>Labeling</span>  Tool</h1>
          <p>Data Science starts with data. <br>
          Label Studio removes the pain labeling it.</p>
          <div class="release__info">
            <a href="http://localhost:4000/blog/release-080-time-series-labeling.html" class="release__info--inner">
              <span>New</span>
              <p>Release <span>0.8.0</span></p>
            </a>
            <p>Time Series is Everywhere!</p>
          </div>
          <div class="commit__info">
            <a href="https://github.com/heartexlabs/label-studio" class="commit--inner" target="_blank">
              <span class="star__span"><img src="<%- url_for("/images/new/staricon.svg") %>" alt="starticon"> Star</span>
              <span class="star__counter loading"></span>
            </a>
            <p class="last__commit">Last commit: <span></span></p>
          </div>
          <script>
          const star_counter = document.querySelector('.star__counter')
          const last_commit = document.querySelector('.last__commit span')
          fetch('https://api.github.com/repos/heartexlabs/label-studio')
          .then(res => res.json())
          .then(res => {
            const date = new Date(res.pushed_at).toLocaleString('en', { year: 'numeric', month: 'short', day: 'numeric' })
            last_commit.textContent = date
            const stars = res.stargazers_count
            if (stars) {
              star_counter.textContent = stars
              star_counter.classList.remove('loading')
            }
          })
          </script>
        </div>
        <div class="right__content">
          <div class="content__switcher">
            <h6>Quick Start</h6>
            <div class="switcher__inner">
              <ul>
                <li class="active__switcher"><a href="#" data-block="content1">PIP</a></li>
